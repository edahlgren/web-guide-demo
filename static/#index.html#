  <!-- Quick start -->
  <div class="section">
    <h2 class="section-title">Quick start</h2>

    <!-- Show how to enter and run the demo -->
    <table class="section-content">
      <tbody>
        <tr>
          <td class="text em">Enter the demo</td>
          <td class="block-code">

<pre><code>$ demo shell dm/ant-colony-search
root@ant-colony-search:#                
</code></pre>

          </td>
        </tr>
        <tr>
          <td class="text em">Run the demo</td>
          <td class="block-code">
            
<pre><code>root@ant-colony-search:# demo run
</code></pre>

          </td>
        </tr>
      </tbody>
    </table> <!-- end of section content -->
  </div> <!-- end of section -->
    
  <!-- Experimenting -->
  <div class="section">
    <h2 class="section-title">Experimenting</h2>

    <table class="section-content">
      <tbody>
        <tr>
          <td class="text em">First</td>
          <td class="text">
            Get familiar with the <a href="">demo shell</a>, a terminal environment that you'll use to interact with the demo
          </td>
        <tr>
        <tr class="hspace"></tr>
        <tr>
          <td class="text em">Then</td>
          <td class="text">
            Try each of the <a href="/command/run.html#configured">preconfigured runs</a> to see a broader range of what the demo can do
          </td>
        <tr>
        <tr class="hspace"></tr>
        <tr>
          <td class="text em">Finally</td>
          <td class="text">
            Experiment with different <a href="/command/run.html#options">command line options</a> and try using different <a href="/command/data.html">data sets</a>
          </td>
        <tr>
      </tbody>
    </table>
  </div>
  
  <!-- Dig deeper -->
  <div class="section">
    <h2 class="section-title">Dig deeper</h2>

    <table class="section-content">
      <tbody>
        <tr>
          <td class="text em">Learn</td>
