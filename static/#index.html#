  <!-- The title of the demo -->
  <h1>Ant Colony Search</h1>
  
  <!-- Layout of the demo -->
  <div class="section">
    <h2 class="section-title">Layout</h2>

    <table class="section-content">
      <tbody class="last-p4">
        <tr>
          <td class="inline-code bold">/shared</td>
          <td class="text">Can be synchronized with a local directory</td>
        </tr>
        <tr class="hspace"></tr>
        <tr>
          <td class="inline-code bold">/demo/setup</td>
          <td class="text">Scripts used to setup the demo</td>
        </tr>
        <tr>
          <td class="inline-code bold">/demo/run</td>
          <td class="text">Scripts for running the demo</td>
        </tr>
        <tr>
          <td class="inline-code bold">/demo/build</td>
          <td class="text">Scripts for building the demo</td>
        </tr>
        <tr>
          <td class="inline-code bold">/demo/docs</td>
          <td class="text">Generated docs</td>
        </tr>
        <tr>
          <td class="inline-code bold">/demo/demo.yml</td>
          <td class="text">Configuration file that fully describes the demo</td>
        </tr>
        <tr class="hspace"></tr>
        <tr>
          <td class="inline-code bold">/root/src</td>
          <td class="text">Source code</td>
        </tr>
        <tr>
          <td class="inline-code bold">/root/data</td>
          <td class="text">Data sets</td>
        </tr>
      </tbody>
    </table> <!-- end of section content -->
  </div> <!-- end of section -->
  
